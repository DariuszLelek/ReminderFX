<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.*?>

<AnchorPane fx:id="rootPane" styleClass="componentRootPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reminder.components.ToDoComponentFXMLController">
   <children>
      <HBox prefHeight="100.0" prefWidth="389.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Pane>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Pane>
            <Label fx:id="txtTaskName" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="labelName" text="To Do task" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <Label maxHeight="1.7976931348623157E308" minWidth="-Infinity" text="Enterd">
               <HBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Label>
            <Label fx:id="txtOverdue" alignment="CENTER" maxHeight="1.7976931348623157E308" minWidth="-Infinity" text="00d 00h">
               <HBox.margin>
                  <Insets bottom="5.0" left="3.0" right="3.0" top="5.0" />
               </HBox.margin>
            </Label>
            <Label maxHeight="1.7976931348623157E308" minWidth="-Infinity" text="ago">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Label>
            <Button maxHeight="1.7976931348623157E308" maxWidth="65.0" minWidth="65.0" mnemonicParsing="false" onAction="#btnDelete" prefWidth="65.0" text="Remove" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <stylesheets>
       <URL value="@ComponentStyles.css" />
   </stylesheets>

</AnchorPane>
